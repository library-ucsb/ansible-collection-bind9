$TTL 3h;
@                       IN  SOA ns1.library.ucsb.edu.  sysadmin.library.ucsb.edu. (
                        	2020062301		; serial number
                        	3h          		; refresh
                        	1h           		; retry
                        	4w      		; expire
                        	1h      ) 		; default TTL

@			IN NS	ns1.library.ucsb.edu.
@			IN NS	ns2.library.ucsb.edu.
@                       IN NS   ns1.ucsb.edu.
@                       IN NS   ns2.ucsb.edu.

